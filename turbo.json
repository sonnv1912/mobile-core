{
   "$schema": "https://turborepo.com/schema.json",
   "ui": "tui",
   "globalEnv": ["JAVA_HOME", "ANDROID_HOME"],
   "tasks": {
      "ios": {
         "cache": false,
         "persistent": true,
         "dependsOn": ["^dev_package"]
      },
      "android": {
         "cache": false,
         "persistent": true,
         "dependsOn": ["^dev_package"]
      },
      "dev_package": {}
   }
}
